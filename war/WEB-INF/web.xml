<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>MyProject</servlet-name>
		<servlet-class>com.t2k.javacourse.MyProjectServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyProject</servlet-name>
		<url-pattern>/myproject</url-pattern>		
	</servlet-mapping>
	<servlet>
		<servlet-name>Exercise3</servlet-name>
		<servlet-class>com.t2k.javacourse.Exercise3</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Exercise3</servlet-name>
		<url-pattern>/exercise3</url-pattern>		
	</servlet-mapping>
	<servlet>
		<servlet-name>StockDetailsServlet</servlet-name>
		<servlet-class>com.t2k.javacourse.StockDetailsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StockDetailsServlet</servlet-name>
		<url-pattern>/exercise4</url-pattern>		
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
